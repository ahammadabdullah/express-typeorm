{
  "name": "express-todo",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "seed": "cross-env NODE_ENV=development ts-node src/database/index.ts",
    "migration:generate": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:generate ./src/database/migrations/CreateNewMigration -d ./src/config/db.ts",
    "migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d ./src/config/db.ts",
    "migration:revert": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert -d ./src/config/db.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cross-env": "^10.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.20",
    "@types/express-serve-static-core": "^4.17.41",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
